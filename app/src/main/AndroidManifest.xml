<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.doan.levanha.pgpmessager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.doan.levanha.pgpmessager.UI.Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.doan.levanha.pgpmessager.UI.Activity.RegisterActivity" />
        <activity android:name="com.doan.levanha.pgpmessager.UI.Activity.LoginActivity" />
        <activity android:name="com.doan.levanha.pgpmessager.UI.Activity.UserListActivity" />
        <activity android:name="com.doan.levanha.pgpmessager.UI.Activity.ChatActivity" />
        <activity android:name="com.doan.levanha.pgpmessager.UI.Activity.ChatPrivateActivity"></activity>
    </application>

</manifest>